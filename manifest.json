{
  "manifest_version": 2,

  "name": "POMO: A Pomodoro Timer",
  "description": "Pomodoro Timer. POMO helps you focus!",
  "version": "0.0.2",
  "icons":{
    "16":"ext/img/icon16.png",
    "128":"ext/img/icon128.png"
  },
  "browser_action":{
   "default_icon": "ext/img/icon16.png"
  },
  "chrome_url_overrides":{
    "newtab":"index.html"
  },
  "background":{
    "scripts":["ext/js/lib/background.js"],
    "persistent":false
  },
  "permissions":["tabs"],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}